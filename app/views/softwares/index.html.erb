<p id="notice"><%= notice %></p>

<h1>Softwares</h1>

<table>
  <thead>
    <tr>
      <th>Nomor serial</th>
      <th>Nama</th>
      <th>Kategori</th>
      <th>License by</th>
      <th>Expired date</th>
      <th>Vendor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @softwares.each do |software| %>
      <tr>
        <td><%= software.nomor_serial %></td>
        <td><%= software.nama %></td>
        <td><%= software.kategori %></td>
        <td><%= software.license_by %></td>
        <td><%= software.expired_date %></td>
        <td><%= software.vendor %></td>
        <td><%= link_to 'Show', software %></td>
        <td><%= link_to 'Edit', edit_software_path(software) %></td>
        <td><%= link_to 'Destroy', software, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Software', new_software_path %>
